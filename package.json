    "@material-ui/icons": "^4.11.2",
    "aws-sdk": "^2.831.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "8.5.1",
    "material-ui": "^0.20.2",
    "mongoose": "^5.3.16",
    "passport": "^0.4.1",
